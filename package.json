{
  "name": "oshop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "forever -w bin/www",
    "exit": "taskkill /im node.exe /f",
    "start_": "pm2 start bin/www",
    "stop": "pm2 stop bin/www",
    "build": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "hmr": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "async": "^2.5.0",
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "currency-formatter": "^1.2.1"
  }
}
