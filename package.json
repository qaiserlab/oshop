{
  "name": "oshop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "forever -w bin/www",
    "exit": "taskkill /im node.exe /f",
    "start_": "pm2 start bin/www",
    "stop": "pm2 stop bin/www",
    "build": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "hmr": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "async": "^2.5.0",
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "currency-formatter": "^1.2.1",
    "express-await": "0.0.4",
    "express-session": "^1.15.5",
    "jsonwebtoken": "^8.2.1",
    "maintenance": "0.0.2",
    "mongoose": "^4.11.5",
    "mongoose-fill": "^1.6.0",
    "mongoose-model-loader": "0.0.6",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "mysql-promise": "^4.1.0",
    "node-schedule": "^1.2.5",
    "node-xlsx": "^0.11.2",
    "pagination": "^0.4.4",
    "sha1": "^1.1.1",
    "shortid": "^2.2.8",
    "slugify": "^1.2.1",
    "striptags": "^3.1.0",
    "vue-template-compiler": "^2.5.2"
  }
}
